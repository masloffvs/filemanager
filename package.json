  "type": "module",
  "scripts": {
    "dev": "if [ ! -d \"node_modules\" ]; then bun install; fi && bun --hot src/index.tsx",
    "start": "NODE_ENV=production bun src/index.tsx",
    "build": "bun run build.ts"
  },
  "dependencies": {
    "bun-plugin-tailwind": "^0.0.15",
