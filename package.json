    "nsfwjs": "^4.2.1",
    "react": "^19",
    "react-dom": "^19",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.11",
    "uniq": "^1.0.1",
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest",
    "@types/yauzl": "latest"
  }
}
